      "get": {
        "tags": [
          "Category"
        ],
        "summary": "Get List category from modifiedDate.",
        "description": "Sample request:\r\n\r\n    GET category/ListByModifiedDate\r\n    {        \r\n      \"firstName\": \"Mike\",\r\n      \"lastName\": \"Andrew\",\r\n      \"emailId\": \"Mike.Andrew@gmail.com\"        \r\n    }",
        "parameters": [
          {
            "name": "modifiedDate",
            "in": "query",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Category"
